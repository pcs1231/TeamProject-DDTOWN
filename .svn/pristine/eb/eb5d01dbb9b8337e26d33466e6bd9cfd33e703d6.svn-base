		  , creply.comu_reply_del_yn
		  , (select count(*) from community_reply core where core.comu_post_no = cpost.comu_post_no) as comu_post_reply_like_cnt
		  , creply.art_group_no as reply_art_group_no
		  , cprofile.comu_profile_no
