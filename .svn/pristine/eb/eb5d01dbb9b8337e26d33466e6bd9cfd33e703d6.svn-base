 	<select id="selectProfile" parameterType="kr.or.ddit.vo.community.CommunityProfileVO" resultMap="profileMap">
 		SELECT
		    cprofile.comu_profile_no
		  , cprofile.art_group_no
		  , cprofile.mem_username
		  , cprofile.comu_mem_cat_code
		  , cprofile.comu_profile_img
		  , cprofile.comu_nicknm
		  , cprofile.comu_reg_date
		  , cprofile.comu_del_yn
		  , cpost.comu_post_no
		  , cpost.board_type_code
		  , cpost.comu_profile_no as post_COMU_PROFILE_NO
		  , cpost.art_group_no as post_ART_GROUP_NO
		  , cpost.file_group_no
		  , cpost.comu_post_stat_code
		  , cpost.comu_post_content
		  , cpost.comu_post_reg_date
		  , cpost.comu_post_mod_date
		  , cpost.comu_post_mbsp_yn
		  , cpost.comu_post_del_yn
		  , creply.comu_reply_no
		  , creply.comu_post_no as reply_COMU_POST_NO
		  , creply.board_type_code
		  , creply.comu_profile_no as reply_COMU_PROFILE_NO
		  , creply.comu_reply_content
		  , creply.comu_reply_reg_date
		  , creply.comu_reply_mod_date
		  , creply.comu_reply_del_yn
		  , creply.art_group_no as reply_art_group_no
		  , (select comu_post_content from community_post comupost where comupost.comu_post_no = creply.comu_post_no) as reply_comu_post_content
		FROM
		    community_profile cprofile
		LEFT OUTER JOIN
		    community_post cpost on (cprofile.comu_profile_no = cpost.comu_profile_no)
		LEFT OUTER JOIN
		    community_reply creply on (cprofile.comu_profile_no = creply.comu_profile_no)
		WHERE
		    cprofile.comu_profile_no = #{comuProfileNo}
		and
		    cprofile.art_group_no = #{artGroupNo}
		and
		    cprofile.comu_del_yn = 'N'
		and
		    (cpost.comu_post_del_yn = 'N' or cpost.comu_post_del_yn is null)
		and
		    (creply.comu_reply_del_yn = 'N' or creply.comu_reply_del_yn is null)
 	</select>

