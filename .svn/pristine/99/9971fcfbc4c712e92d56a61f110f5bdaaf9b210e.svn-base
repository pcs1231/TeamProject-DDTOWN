				singleGoodsName: "DDTOWN ì•„í‹°ìŠ¤íŠ¸ ë©¤ë²„ì‹­",
				totalAmount: 30000,
				isFromCart: false,
				orderTypeCode: "OTC001", // ë©¤ë²„ì‹­
				orderPayMethodNm: "ì¹´ì¹´ì˜¤í˜ì´",
